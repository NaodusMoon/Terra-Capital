{
  "name": "terra-capital-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:web": "next dev",
    "dev:offchain": "scripts\\dev-offchain.cmd",
    "dev:all": "concurrently -k -n WEB,OFFCHAIN -c green,cyan \"npm run dev:web\" \"npm run dev:offchain\"",
    "build": "next build && opennextjs-cloudflare build --skipBuild",
    "start": "next start",
    "lint": "eslint",
    "cf:check": "node scripts/verify-cloudflare-config.mjs",
    "cf:build": "npm run cf:check && npm run build",
    "cf:preview": "npm run cf:build && wrangler dev",
    "cf:deploy": "npm run cf:build && wrangler deploy",
    "cf:d1:migrate": "wrangler d1 migrations apply tera_d1 --local",
    "cf:d1:migrate:remote": "wrangler d1 migrations apply tera_d1 --remote"
  },
  "dependencies": {
    "@albedo-link/intent": "^0.13.0",
    "@stellar/freighter-api": "^6.0.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.33.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "p5": "^2.2.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "vanta": "^0.5.24"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260207.0",
    "@opennextjs/cloudflare": "^1.16.3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.63.0"
  }
}
